pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building your application ReactV18..'
                echo 'Jenkins running sucessfully ReactV18... '
            }
        }
        stage('Test') {
            steps {
                echo 'Testing your application ReactV18..'
                echo 'Jenkins testing sucessfully ReactV18... '
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying your application....'
                echo 'Running jenkins inside deploy section of my application....'
            }
        }
    }
}
